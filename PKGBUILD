pkgname=one-font
pkgver=1.0
pkgrel=2

pkgdesc="OTF font with vertical bars for one-line ASCII spectrum analyzers, graphs, etc"

arch=('any')
url="https://github.com/JuckZ/one-font"
license=('MIT')

depends=()
makedepends=()
optdepends=()

source=('https://github.com/JuckZ/one-font/blob/xxxxx/One-Font.otf')
md5sums=('xxxxxxxxxxxxx')

package() {
    install -Dm644 *.otf -t "${pkgdir}/usr/share/fonts/OTF/"
}
